{
  "title": "Matching System Dashboard",
    "tags": ["matching", "poc"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Queue Length",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "matching_match_queue_length",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Match Success Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "(rate(matching_match_success_count[1m]) / clamp_min(rate(matching_match_success_count[1m]) + rate(matching_match_fail_count[1m]), 0.0001)) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Worker Tick Latency",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "rate(matching_worker_tick_latency_sum[1m]) / rate(matching_worker_tick_latency_count[1m])",
            "refId": "A",
            "legendFormat": "Average"
          },
          {
            "expr": "histogram_quantile(0.95, rate(matching_worker_tick_latency_bucket[1m]))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(matching_worker_tick_latency_bucket[1m]))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "Redis ZRANGE Latency",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "rate(matching_redis_zrange_latency_sum[1m]) / rate(matching_redis_zrange_latency_count[1m])",
            "refId": "A",
            "legendFormat": "Average"
          },
          {
            "expr": "histogram_quantile(0.95, rate(matching_redis_zrange_latency_bucket[1m]))",
            "refId": "B",
            "legendFormat": "P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "Redis Lua Script Latency",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 4},
        "targets": [
          {
            "expr": "rate(matching_redis_lua_latency_sum[1m]) / rate(matching_redis_lua_latency_count[1m])",
            "refId": "A",
            "legendFormat": "Average"
          },
          {
            "expr": "histogram_quantile(0.95, rate(matching_redis_lua_latency_bucket[1m]))",
            "refId": "B",
            "legendFormat": "P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Match Success/Fail Count",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "rate(matching_match_success_count[1m])",
            "refId": "A",
            "legendFormat": "Success"
          },
          {
            "expr": "rate(matching_match_fail_count[1m])",
            "refId": "B",
            "legendFormat": "Fail"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "palette-classic"}
          }
        }
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
  "annotations": {
    "list": []
  }
}

